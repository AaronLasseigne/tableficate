<% if table.filters.any? %>
  <%= tableficate_filter_form_for(table) %>
<% end %>

<% if defined? ::Kaminari %>
  <%= paginate(table.rows, param_name: "#{table.param_namespace}_page") %>
<% elsif defined? ::WillPaginate %>
  <%= will_paginate(table.rows, param_name: "#{table.param_namespace}_page") %>
<% end %>

<%= tableficate_table_tag(table) %>
